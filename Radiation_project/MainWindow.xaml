                    <GroupBox Margin="10,181,332,117.6">
                        <GroupBox.Header>Componentes</GroupBox.Header>
                        <StackPanel Margin="0,0,9,2">
                            <ScrollViewer Height="180" >
                                <ListBox x:Name="cmbComponentes" >
